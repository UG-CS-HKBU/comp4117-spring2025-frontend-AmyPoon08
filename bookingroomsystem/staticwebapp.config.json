{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "mimeTypes": {
        ".json": "text/json"
    },
    "routes": [
        {
            "route": "/api/logout",
            "methods": ["POST", "OPTIONS"],
            "rewrite": "https://roombookingsystem-etc7bfeeg8hndfbc.eastasia-01.azurewebsites.net/api/logout",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/api/*",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "rewrite": "https://roombookingsystem-etc7bfeeg8hndfbc.eastasia-01.azurewebsites.net/api/{rest}",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "globalHeaders": {
        "Access-Control-Allow-Origin": "https://happy-bay-03dd73d00.6.azurestaticapps.net",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true"
    }
}
