{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "mimeTypes": {
        ".json": "text/json"
    },
    "routes": [
        {
            "route": "/api/*",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "rewrite": "/api/index"
        },
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ]
}
